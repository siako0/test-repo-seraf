    affectedPackage: linux
    affectedVersion:
      fullName: 6.1.129-1
      kind: NORMAL
      name: 6.1.129
      revision: '1'
    fixedCpeUri: cpe:/o:debian:debian_linux:12
    fixedPackage: linux
    fixedVersion:
      kind: MAXIMUM
